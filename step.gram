expr ::= term '+' expr
       | term '-' expr
       | term

term ::= factor '*' term
       | factor '/' term
       | factor

factor ::= number
         | '(' expr ')'

number ::= inetger
         | float

integer ::= [0-9]([0-9]*_*[0-9])*

float ::= integer '.' integer ('E' | 'e')? ('+' | '-')? integer
