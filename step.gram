expression ::= expression '+' term
             | expression '-' term
             | term

term ::= term '*' factor
       | term '/' factor
       | factor

factor ::= literal
         | '(' expression ')'

literal ::= inetger
         | float

integer ::= [0-9]([0-9]*_*[0-9])*

float ::= integer '.' integer ('E' | 'e')? ('+' | '-')? integer
