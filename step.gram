expression ::= term '+' expression
             | term '-' expression
             | term

term ::= factor '*' term
       | factor '/' term
       | factor

factor ::= literal
         | '(' expression ')'

literal ::= inetger
         | float

integer ::= [0-9]([0-9]*_*[0-9])*

float ::= integer '.' integer ('E' | 'e')? ('+' | '-')? integer
